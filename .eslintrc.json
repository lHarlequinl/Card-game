{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": ["eslint:recommended", "prettier"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 2022
    },
    "rules": {
        "prettier/prettier": "error",
        "snakecase": ["error"],
        // "eqeqeq": ["error", "always"]
    }
}
